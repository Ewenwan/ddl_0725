<launch>
    <include file="$(find common)/launch/static_tf.launch" />
    <include file="$(find lslidar_c16_decoder)/launch/lslidar_c16.launch" />
    <include file="$(find hall_sensor)/launch/hall_sensor.launch" />
    <include file="$(find imu_driver)/launch/imu_driver.launch" />
    <node pkg="imu_complementary_filter" type="complementary_filter_node" name="complementary_filter_gain_node" output="screen">
        <param name="fixed_frame" value="/imu_link" />
        <param name="use_mag" value="false"/>
    </node>

    <include file="$(find ndt_localization)/launch/test_ndt.launch" />
    <node pkg="map_server" type="map_server" name="gridmap_server" args="$(rospack find static_map)/map_file/gridmap_1008.yaml">
    </node>
    <include file="$(find teb_local_planner)/launch/test_main.launch" />
</launch>